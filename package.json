{
  "name": "typed-scss-modules",
  "version": "0.1.0",
  "description": "TypeScript type definition generator for SCSS CSS Modules",
  "main": "index.js",
  "author": "Spencer Miskoviak <smiskoviak@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/skovy/typed-scss-modules.git"
  },
  "homepage": "https://github.com/skovy/typed-scss-modules.git#readme",
  "keywords": [
    "scss",
    "css modules",
    "cli",
    "typescript",
    "type generator",
    "scss modules"
  ],
  "scripts": {
    "test": "jest",
    "tsm": "ts-node ./lib/cli.ts",
    "clean": "rimraf -rf ./dist",
    "build": "yarn clean && tsc",
    "prepack": "yarn build",
    "check-types": "tsc --noEmit",
    "check-formatting": "prettier --check **/*.{js,json,css,md,scss,tsx,ts}",
    "postinstall": "yarn build"
  },
  "files": [
    "dist/lib"
  ],
  "bin": {
    "tsm": "./dist/lib/cli.js"
  },
  "devDependencies": {
    "@types/camelcase": "^5.2.0",
    "@types/css-modules-loader-core": "^1.1.0",
    "@types/glob": "^7.1.1",
    "@types/jest": "^24.0.18",
    "@types/node-sass": "^4.11.0",
    "@types/param-case": "^1.1.2",
    "@types/yargs": "^13.0.3",
    "husky": "^3.0.8",
    "jest": "^24.9.0",
    "lint-staged": "^9.4.2",
    "node-sass": "^4.12.0",
    "prettier": "^1.18.2",
    "ts-jest": "^24.1.0",
    "ts-node-dev": "^1.0.0-pre.43",
    "typescript": "^3.6.3"
  },
  "peerDependencies": {
    "node-sass": "^4.12.0"
  },
  "dependencies": {
    "@types/reserved-words": "^0.1.0",
    "camelcase": "^5.3.1",
    "chalk": "^2.4.2",
    "chokidar": "^3.2.1",
    "css-modules-loader-core": "^1.1.0",
    "glob": "^7.1.4",
    "param-case": "^2.1.1",
    "path": "^0.12.7",
    "reserved-words": "^0.1.2",
    "slash": "^3.0.0",
    "yargs": "^14.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md,scss,tsx,ts}": [
      "prettier --write",
      "git add"
    ]
  }
}
